{
  "builds": [
    {
      "src": "xradioproject/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "xradioproject/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12"
    }
  }
}
